<article class="post-info">
  <h1 class="post-title"><%= page.title %></h1>
  <%- partial('partials/meta', { post: page, class_name: 'post-meta' }) %>
</article>

<article class="post">
  <%- partial('partials/post-header', { post: page }) %>
  <div class="post-content">
    <%-page.content%>
  </div>
  <%- partial('partials/tags', { post: page, class_name: 'post-tags' }) %>

</article>

<%- partial('partials/navigation', { post: page }) %>
<%- partial('partials/comment') %>
<%- partial('partials/author') %>

<script>window.pageType = 'post';</script>